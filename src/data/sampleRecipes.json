    "description": "经典的红烧肉，肥瘦相间，入口即化",
    "category": "荤菜",
    "difficulty": "中等",
    "servings": 4,
    "prepTime": "20分钟",
    "cookTime": "1小时30分钟",
    "totalTime": "1小时50分钟",
    "ingredients": [
      { "name": "五花肉", "amount": "500g", "notes": "切成2cm见方的块" },
      { "name": "冰糖", "amount": "30g" },
      { "name": "生抽", "amount": "2汤匙" },
      { "name": "老抽", "amount": "1汤匙" },
      { "name": "料酒", "amount": "2汤匙" },
      { "name": "葱", "amount": "2根", "notes": "切段" },
      { "name": "姜", "amount": "3片" },
      { "name": "八角", "amount": "2个" },
      { "name": "桂皮", "amount": "1小段" }
    ],
    "steps": [
      { "stepNumber": 1, "instruction": "五花肉洗净切块，冷水下锅焯水去腥", "duration": "5分钟" },
      { "stepNumber": 2, "instruction": "热锅下肉块，小火煸炒出油", "duration": "10分钟" },
      { "stepNumber": 3, "instruction": "加入冰糖炒糖色，炒至肉块上色", "duration": "5分钟" },
      { "stepNumber": 4, "instruction": "加入生抽、老抽、料酒翻炒均匀", "duration": "2分钟" },
      { "stepNumber": 5, "instruction": "加入葱段、姜片、八角、桂皮", "duration": "1分钟" },
      { "stepNumber": 6, "instruction": "加开水没过肉块，大火烧开转小火炖煮", "duration": "1小时" },
      { "stepNumber": 7, "instruction": "大火收汁，汤汁浓稠即可", "duration": "5分钟" }
    ],
    "tags": ["经典", "下饭", "家常菜"],
    "cookingMethods": ["炒", "炖"],
    "nutritionalInfo": { "calories": 450 },
